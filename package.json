{
  "name": "pokedex-api",
  "version": "1.0.0",
  "description": "API Pokedex avec ORM, cache Redis et intégration météo",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "dev": "nodemon main.js",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "_moduleAliases": {
    "@root": ".",
    "@app": "./app",
    "@configs": "./configs",
    "@models": "./models",
    "@controllers": "./controllers",
    "@routes": "./routes",
    "@services": "./services",
    "@utils": "./utils",
    "@middlewares": "./middlewares"
  },
  "keywords": [
    "pokedex",
    "pokemon",
    "api",
    "redis",
    "weather"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}
